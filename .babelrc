{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": "3",
        "targets": {
          "browsers": ["> 0.25%, not dead"]
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "throwIfNamespace": false,
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "babel-plugin-styled-components"
  ]
}
